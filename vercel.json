{
    "version": 2,
    "builds" :[
        {
        "src": "./index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },        
        {
            "src": "/login",
            "dest": "/clientes"
        },
        {
            "src": "/clientes",
            "dest": "/update"
        },
        {
            "src": "/vehiculos",
            "dest": "/contacto"
        }
    ]
}